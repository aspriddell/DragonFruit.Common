os: windows
language: shell

before_script:
  - choco install dotnetcore-sdk
  - |
    set year=%date:~10,4%
    set month=%date:~4,2%
    set day=%date:~7,2%
    set filename=%year%.%month%.%day%

script:
  - dotnet restore ./*.sln
  - dotnet publish ./*.sln -f netstandard2.0 -c Release -p:Version=%filename%
